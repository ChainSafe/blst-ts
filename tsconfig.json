{
  "compilerOptions": {
    "noEmit": true,
    "lib": [
      "ESNext",
      "ESNext.BigInt",
    ],
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": true,
    "pretty": true,
  }
}